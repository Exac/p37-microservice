{
  "name": "frontend2",
  "version": "1.0.0",
  "description": "Frontend",
  "main": "./dist/src/index.js",
  "scripts": {
    "test": "nyc mocha --exit",
    "test:watch": "nyc mocha --watch",
    "test:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "start:debug": "node --inspect -r ts-node/register src/index.ts",
    "start:watch": "tsnd --respawn --transpileOnly src/index.ts",
    "start": "npm run start:watch",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "lint": "tsc --noEmit && eslint src/**/*.ts --quiet --fix",
    "config": "node-config-ts"
  },
  "keywords": [
    "frontend"
  ],
  "author": "Thomas McLennan",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/sinon": "^7.0.11",
    "@types/sinon-chai": "^3.2.2",
    "@types/sinon-express-mock": "^1.3.7",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "mocha": "^6.1.4",
    "morgan": "^1.9.1",
    "node-config-ts": "^2.2.4",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "sinon-express-mock": "^2.2.0",
    "winston": "^3.2.1"
  }
}
